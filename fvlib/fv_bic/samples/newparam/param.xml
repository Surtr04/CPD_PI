<?xml version="1.0" encoding="ISO-8859-1"?>
<fvlib>
	<!-- parameter list must consist of a <parameters> tag with all params inside -->
	<parameters>
		<!-- tag name indicates param name. if no key is given, it defaults to "value" -->
		<!-- for this first param, param.getString("StringVal") will return "mesh.xml" -->
		<!--   while param.getString("StringVal", "opt") will return "string" -->
		<StringVal value="mesh.xml" opt="string" />
		<DoubleVal value="12.3e+2" opt="double" />
		<IntVal    value="-123" opt="int" />
		<UnsignedVal value="123" opt="unsigned" />
		<BoolVal value="1" opt="bool" />
	</parameters>
</fvlib>
